{
  "name": "tenantra-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:ci": "npm run build && npm run bundle:check",
    "build:report": "node ./scripts/build-report.mjs",
    "bundle:check": "node ./scripts/check-bundle.mjs",
    "dev:ssr": "node ./server/devServer.mjs",
    "build:module-catalog-remote": "vite build --config vite.catalog-remote.config.js",
    "build:ssr": "vite build --config vite.ssr.config.js",
    "typecheck": "tsc --noEmit",
    "ssr:serve": "node ./server/prodServer.mjs",
    "preview": "vite preview --host",
    "test:e2e": "playwright test",
    "test:lighthouse": "node ./scripts/run-lighthouse.mjs"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "@tanstack/react-query": "^5.59.16",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "6.26.2",
    "react-router-dom": "6.26.2",
    "web-vitals": "^4.2.4",
    "zod": "^3.23.8",
    "zustand": "^4.5.4",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/forms": "^0.5.9",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "vite": "^5.2.9",
    "@playwright/test": "^1.48.0",
    "@playwright/experimental-ct-react": "^1.48.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "express": "^4.19.2",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "vite-plugin-pwa": "^0.17.5",
    "lighthouse": "^12.1.0",
    "typescript": "^5.6.3"
  },
  "overrides": {
    "esbuild": "^0.25.1",
    "vite": {
      "esbuild": "^0.25.1"
    }
  }
}
